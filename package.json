    "@jspsych/plugin-webgazer-calibrate": "^1.0.3",
    "@jspsych/plugin-webgazer-init-camera": "^1.0.3",
    "@jspsych/plugin-webgazer-validate": "^1.0.3",
    "jspsych": "^7.3.4",
    "pinia": "^2.1.7",
    "pixi.js": "^7.4.0",
    "three": "^0.161.0",
    "vue": "^3.4.19",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "electron": "^29.1.4",
    "electron-builder": "^24.13.3",
    "electron-packager": "^17.1.2",
    "vite": "^5.1.4"
  }
}
